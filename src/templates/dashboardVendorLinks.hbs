<p class="h4 editOption">Vendor Links</p>
<div id="{{idPrefix}}VendorLinks">
	{{#each vendorLinks}}
	<div class="row mb-3 vendor-links editOption">
		<div class="col form-floating">
			<input type="text" class="form-control vendor-name" id="{{../idPrefix}}VendorName{{@index}}" value="{{this.name}}">
			<label for="{{../idPrefix}}VendorName{{@index}}">Vendor Name</label>
		</div>
		<div class="col form-floating">
			<input type="text" class="form-control vendor-link" id="{{../idPrefix}}VendorLink{{@index}}" value="{{this.link}}">
			<label for="{{../idPrefix}}VendorLink{{@index}}">Vendor Link</label>
		</div>
	</div>
	{{/each}}
</div>